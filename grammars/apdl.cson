                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/ui)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(help|view|wpse|wpvi|result|query|copy|anno|an3d|select|nsel|esel|ksel|lsel|asel|vsel|refresh|coll|s|r|a|u|node|element|yes|no|pscr|tiff|epsi|bmp|wmf|emf|jpeg|full|graph|mono|gray|color|norm|reverse|landscape|portrait|yes|no)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/uis)\\b([^!]*)"
