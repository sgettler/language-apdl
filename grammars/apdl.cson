# ANSYS APDL grammar based on Release 15.0 Mechanical APDL documentation

name: "ANSYS APDL"
scopeName: "source.apdl"
fileTypes: [
    "mac"
    "ans"
    "inp"
]



patterns: [

    # comments
    {
        match: "!.*$"
        name: "comment.line.apdl"
    }

    # strings
    {
        begin: "\'"
        end: "\'"
        name: "string.quoted.single.apdl"
    }


    # commands
    { include: "#apdl_session_commands" }
    { include: "#apdl_database_commands" }
    { include: "#apdl_graphics_commands" }
    { include: "#apdl_apdl_commands" }
    # { include: "#apdl_prep7_commands" }
    # { include: "#apdl_solution_commands" }
    # { include: "#apdl_post1_commands" }
    # { include: "#apdl_post26_commands" }

    # functions
    { include: "#apdl_functions" }

    # element types
    { include: "#apdl_elements" }

]



repository:

    # session commands; see Command Reference 2.1
    apdl_session_commands: {
        patterns: [
            { include: "#apdl_session_run_commands" }
            { include: "#apdl_session_processor_commands" }
            { include: "#apdl_session_file_commands" }
            { include: "#apdl_session_list_commands" }
        ]
    }

    apdl_session_run_commands: {
        patterns: [
            {
                match: "^[ \\t]*(?i)(\/cwd|\/eof|help|\/input|keyw|pause|\/syp|\/sys|unpause)\\b"
                name: "keyword.control.commands.apdl"
            }
            {
                match: "^[ \\t]*(?i)(\/batch)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(list)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/config)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(norstgm|nbuf|locfl|szbio|order|fsplit|mxnd|mxnd|mxel|mxkp|mxls|mxar|mxvl|mxrl|mxcp|mxce|noeldb|dyna_dbl|stat)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/exit)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(model|solu|all|nosave)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/filname)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(off|on)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(memm)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(on|off)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/menu)\\b([^!]*)"
