                    2: patterns: [
                        {
                            match: "\\b(?i)(zoom|work|wpset|abbr|parm|sele|anno|hard|util|off|on)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\/nerr)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(off|on)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                        { include: "$self" }
                    ]
            }
            {
