# ANSYS APDL grammar based on Release 15.0 Mechanical APDL documentation

name: "ANSYS APDL"
scopeName: "source.apdl"
fileTypes: [
    "mac"
    "ans"
    "inp"
]



patterns: [

    # comments
    {
        name: "comment.line.apdl"
        match: "(!|^[ \\t]*(?i)(\/com|c\\*\\*\\*)[ \\t]*,).*$"
    }

    # strings
    {
        name: "string.quoted.single.apdl"
        match: "(\')(.*)(\')"
    }


    # commands
    { include: "#apdl_apdl_commands" }


    # element types
    { include: "#apdl_elements" }

]



repository:

    # APDL commands; see Command Reference 2.4
    apdl_apdl_commands: {
        patterns: [
            { include: "#apdl_apdl_parameter_commands" }
            { include: "#apdl_apdl_macro_commands" }
            { include: "#apdl_apdl_abbreviation_commands" }
            { include: "#apdl_apdl_array_commands" }
            { include: "#apdl_apdl_process_commands" }
            { include: "#apdl_apdl_matrix_commands" }
        ]
    }

    apdl_apdl_parameter_commands: {
        patterns: [
            {
                match: "^[ \\t]*(?i)(\\*ask|\\*set|\\*taxis|\\*tread|\\*vread)\\b"
                name: "keyword.control.commands.apdl"
            }
            {
                match: "^[ \\t]*(?i)(\\*afun)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(rad|deg|stat)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\\*del)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(all|_prm|prm_)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\\*dim)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        {
                            match: "\\b(?i)(array|arr4|arr5|char|table|tab4|tab5|string)\\b"
                            name: "keyword.control.arguments.apdl"
                        }
                    ]
            }
            {
                match: "^[ \\t]*(?i)(\\*get)\\b([^!]*)"
                captures:
                    1: name: "keyword.control.commands.apdl"
                    2: patterns: [
                        # items appearing in multiple contexts
                        {
                            match: "\\b(?i)(active)\\b(.*)"
                            captures:
                                1: name: "keyword.control.arguments.apdl"
                                2: patterns: [
                                    # general
                                    {
                                        match: "\\b(?i)(int|imme|menu|prkey|units|rout|time|wall|cpu|dbase|ldate|dbase|ltime|rev|title|jobnam|platform|nproc|curr|max|maxp|numcpu)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                    # preprocessing
                                    {
                                        match: "\\b(?i)(seg|csys|dsys|mat|type|real|esys|sect|cp|ce|wfront|max|rms)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                    # solution
                                    {
                                        match: "\\b(?i)(anty|solu|dtime|ncmls|ncmss|eqit|ncmit|cnvg|mxdvl|resfrq|reseig|dsprm|focv|mocv|hfcv|mfcv|cscv|cucv|ffcv|dicv|rocv|tecv|vmcv|smcv|vocv|prcv|vecv|crprat|psinc|cgiter)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                    # postprocessing
                                    {
                                        match: "\\b(?i)(set|lstp|sbst|time|freq|nset|first|last|rsys)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                ]
                        }
                        {
                            match: "\\b(?i)(elem)\\b(.*)"
                            captures:
                                1: name: "keyword.control.arguments.apdl"
                                2: patterns: [
                                    # preprocessing
                                    {
                                        match: "\\b(?i)(node|cent|x|y|z|adj|attr|leng|lproj|x|y|z|area|aproj|x|y|z|volu|esel|nxth|nxtl|hgen|dgen|hcoe|tbulk|pres|shpar|num|max|min|num|maxd|mind|count)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                    # solution
                                    {
                                        match: "\\b(?i)(mtot|x|y|z|mc|x|y|z|ior|x|y|z|xy|yz|zx|imc|x|y|z|xy|yz|zx|iprin|x|y|ziang|xy|yz|zx|fmc|x|y|z|mmor|x|y|z|mmmc|x|y|z)\\b"
                                        name: "keyword.control.arguments.apdl"
                                    }
                                    # postprocessing
